{
  "manifest_version": 3,
  "name": "SFChronicle Popup Unlock",
  "short_name": "SFChronicle Popup Unlock",
  "version": "1.0.0",
  "description": "Web Extension for removing intrusive popups on San Francisco Chronicle website.",
  "homepage_url": "https://github.com/Dalimil",
  "icons": {
    "32": "assets/icons/32x32.png",
    "48": "assets/icons/48x48.png",
    "128": "assets/icons/128x128.png"
  },

  "permissions": [],

  "__chrome|firefox__author": "Dalimil Hajek",
  "__opera__developer": {
    "name": "Dalimil Hajek"
  },
  "__chrome__minimum_chrome_version": "97",
  "__opera__minimum_opera_version": "36",
  "content_scripts": [
    {
      "matches": ["https://sfchronicle.com/*", "https://www.sfchronicle.com/*"],
      "js": ["js/contentScript.bundle.js"]
    }
  ]
}
